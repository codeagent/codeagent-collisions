{
  "compileOnSave": false,
  "compilerOptions": {
    "removeComments": false,
    "baseUrl": "./",
    "lib": ["es2020", "es2017", "dom"],
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "ES2018",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": false
  },
  "exclude": ["./node_modules"]
}
