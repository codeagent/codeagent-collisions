{
  "name": "js-physics-2d",
  "version": "0.0.0-alpha",
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/bundle/js-physics-2d.js",
  "typings": "./dist/typings/index.d.ts",
  "author": "i.aleksey.yakovlev@gmail.com",
  "description": "2D rigid body physics for JavaScript and Typescript",
  "license": "MIT",
  "tags": [
    "physics 2D",
    "constraints",
    "rigid-body",
    "javascript physics 2D"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codeagent/js-physics-2d.git"
  },
  "scripts": {
    "build:clean": "npx rimraf dist",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "build:esm5": "npx tsc -p tsconfig.esm5.json",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:types": "npx tsc -p tsconfig.types.json",
    "build:bundle": "npx rollup -c --bundleConfigAsCjs",
    "start": "npx rollup -c --watch --bundleConfigAsCjs --environment BUILD:development",
    "test": "npx jest"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/jest": "^29.2.3",
    "generate-license-file": "^2.0.0",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.3.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rxjs": "^7.5.7",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/gl-matrix": "^3.2.0",
    "gl-matrix": "^3.4.3",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  }
}
