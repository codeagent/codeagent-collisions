{
  "name": "rb-phys2d",
  "version": "0.0.1-alpha",
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/types/index.d.ts",
  "author": "i.aleksey.yakovlev@gmail.com",
  "description": "2D rigid body physics for JavaScript and Typescript",
  "license": "MIT",
  "tags": [
    "physics 2D",
    "constraints",
    "rigid-body",
    "javascript physics 2D"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codeagent/rb-phys2d.git"
  },
  "files": [
    "dist/bundle/*",
    "dist/cjs/*",
    "dist/esm/*",
    "dist/types/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "format:check": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "build:clean": "npx rimraf dist",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:types": "npx tsc -p tsconfig.types.json",
    "build:bundle": "npx rollup -c --bundleConfigAsCjs",
    "compile:examples": "node ./scripts/compile-examples.mjs",
    "start": "npx rollup -c --watch --bundleConfigAsCjs --environment BUILD:development",
    "test": "npx jest"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "generate-license-file": "^2.0.0",
    "jest": "^29.3.1",
    "prettier": "2.8.3",
    "rimraf": "^3.0.2",
    "rollup": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rxjs": "^7.5.7",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/gl-matrix": "^3.2.0",
    "gl-matrix": "^3.4.3",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0"
  }
}
